    "@vue/compiler-sfc": "^3.0.5",
    "cross-env": "^7.0.3",
    "less": "^4.1.1",
    "less-loader": "^9.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "typescript": "^4.1.3",
    "vite": "^2.3.4",
